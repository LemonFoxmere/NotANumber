{
  "hosting": [
    {
      "site": "interactive-dcgan",
      "public": "./public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/raw/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "hosting": {
      "host": "localhost",
      "port": "4998"
    }
  }
}
